    "html-react-parser": "^5.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-redux": "^8.1.3",
    "react-redux-loading-bar": "^5.0.8",
